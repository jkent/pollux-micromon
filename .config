
#
# Baremetal: startup
#
CONFIG_BAREMETAL_BOOT_SOURCE_RAM=y
CONFIG_BAREMETAL_SHADOW=y
CONFIG_BAREMETAL_LOAD_ADDRESS=0x00800000
CONFIG_BAREMETAL_RELOCATE=y
CONFIG_BAREMETAL_ENTRY_ADDRESS=0x00000000
CONFIG_BAREMETAL_STACK_SIZE=0x40000
CONFIG_BAREMETAL_ENABLE_ICACHE=y
CONFIG_BAREMETAL_ENABLE_DCACHE=y
CONFIG_BAREMETAL_DDR_INIT=y
# CONFIG_BAREMETAL_PLL0_INIT_NONE is not set
CONFIG_BAREMETAL_PLL0_INIT_533=y
CONFIG_BAREMETAL_PLL0_INIT=y
# end of Baremetal: startup

#
# Baremetal: runtime
#
CONFIG_BAREMETAL_USE_NEWLIB=y
CONFIG_BAREMETAL_HEAP_SIZE=0x40000
CONFIG_BAREMETAL_UART_BASIC=y
# CONFIG_BAREMETAL_UART_NONE is not set
CONFIG_BAREMETAL_UART=y
CONFIG_BAREMETAL_CACHE=y
CONFIG_BAREMETAL_CLOCKING=y
# end of Baremetal: runtime
